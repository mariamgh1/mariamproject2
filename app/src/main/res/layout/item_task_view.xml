<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/itmTvTitle"
        android:layout_width="143dp"
        android:layout_height="40dp"
        android:text="TextView"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="39dp" />

    <TextView
        android:id="@+id/itemTvSubject"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="192dp"
        tools:layout_editor_absoluteY="228dp" />

    <TextView
        android:id="@+id/itemTvImportant"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="307dp"
        tools:layout_editor_absoluteY="359dp" />

    <TextView
        android:id="@+id/itemTvNecessary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="248dp" />

    <ImageView
        android:id="@+id/itmbtnCall"
        android:layout_width="88dp"
        android:layout_height="66dp"
        app:srcCompat="@android:drawable/stat_sys_phone_call"
        tools:layout_editor_absoluteX="39dp"
        tools:layout_editor_absoluteY="293dp"
        tools:srcCompat="@android:drawable/ic_menu_call" />

    <ImageView
        android:id="@+id/itmbtnDelete"
        android:layout_width="91dp"
        android:layout_height="61dp"
        app:srcCompat="@android:drawable/ic_menu_delete"
        tools:layout_editor_absoluteX="137dp"
        tools:layout_editor_absoluteY="79dp" />

    <ImageView
        android:id="@+id/itmbtnEdit"
        android:layout_width="109dp"
        android:layout_height="93dp"
        app:srcCompat="@android:drawable/ic_menu_edit"
        tools:layout_editor_absoluteX="170dp"
        tools:layout_editor_absoluteY="352dp" />

</androidx.constraintlayout.widget.ConstraintLayout>